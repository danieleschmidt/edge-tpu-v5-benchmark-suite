# Edge TPU v5 Benchmark Suite Environment Variables
# Copy this file to .env and customize the values for your environment

# =============================================================================
# DEVELOPMENT SETTINGS
# =============================================================================

# Enable development mode with additional logging and debugging features
EDGE_TPU_DEV_MODE=1

# Set logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
EDGE_TPU_LOG_LEVEL=INFO

# Directory for caching models, results, and temporary files
EDGE_TPU_CACHE_DIR=~/.edge-tpu-v5-benchmark/cache

# Enable performance profiling for development
EDGE_TPU_PROFILE_ENABLED=false

# =============================================================================
# TPU DEVICE CONFIGURATION
# =============================================================================

# Path to Edge TPU device (auto-detected if not specified)
EDGE_TPU_DEVICE_PATH=/dev/apex_0

# Alternative device paths (comma-separated for multi-device setups)
# EDGE_TPU_DEVICE_PATHS=/dev/apex_0,/dev/apex_1

# TPU compiler timeout in seconds
EDGE_TPU_COMPILER_TIMEOUT=300

# Enable TPU thermal monitoring
EDGE_TPU_THERMAL_MONITORING=true

# Maximum TPU temperature threshold (Â°C)
EDGE_TPU_MAX_TEMPERATURE=85

# =============================================================================
# BENCHMARK CONFIGURATION
# =============================================================================

# Default number of benchmark iterations
BENCHMARK_DEFAULT_ITERATIONS=1000

# Number of warmup iterations before measurement
BENCHMARK_WARMUP_ITERATIONS=100

# Benchmark timeout per model in seconds
BENCHMARK_TIMEOUT=600

# Output directory for benchmark results
BENCHMARK_OUTPUT_DIR=./results

# Result format (json, csv, html, all)
BENCHMARK_OUTPUT_FORMAT=json

# Enable automatic result archiving
BENCHMARK_ARCHIVE_RESULTS=true

# =============================================================================
# MODEL MANAGEMENT
# =============================================================================

# Directory for downloaded and cached models
MODEL_CACHE_DIR=~/.edge-tpu-v5-benchmark/models

# Model download timeout in seconds
MODEL_DOWNLOAD_TIMEOUT=300

# Enable model validation after conversion
MODEL_VALIDATION_ENABLED=true

# Model optimization level (0=none, 1=basic, 2=standard, 3=aggressive)
MODEL_OPTIMIZATION_LEVEL=2

# =============================================================================
# POWER MEASUREMENT
# =============================================================================

# Enable power measurement during benchmarks
POWER_MEASUREMENT_ENABLED=true

# Power sampling rate in Hz (samples per second)
POWER_SAMPLING_RATE=1000

# Power measurement device (auto-detected if not specified)
# POWER_MEASUREMENT_DEVICE=/dev/ttyUSB0

# Calibration offset for power measurements (watts)
POWER_CALIBRATION_OFFSET=0.0

# =============================================================================
# PERFORMANCE ANALYSIS
# =============================================================================

# Enable compiler analysis and optimization insights
COMPILER_ANALYSIS_ENABLED=true

# Enable bottleneck detection and recommendations
BOTTLENECK_ANALYSIS_ENABLED=true

# Percentiles to calculate for latency analysis (comma-separated)
LATENCY_PERCENTILES=50,90,95,99

# Enable memory usage profiling
MEMORY_PROFILING_ENABLED=true

# =============================================================================
# LEADERBOARD INTEGRATION (OPTIONAL)
# =============================================================================

# Enable leaderboard result submission
LEADERBOARD_SUBMISSION_ENABLED=false

# Leaderboard API endpoint
LEADERBOARD_API_URL=https://api.edge-tpu-benchmark.org

# API key for leaderboard submission (get from leaderboard website)
LEADERBOARD_API_KEY=your_api_key_here

# Include system information in submissions
LEADERBOARD_INCLUDE_SYSTEM_INFO=true

# Automatically submit results after benchmarks
LEADERBOARD_AUTO_SUBMIT=false

# =============================================================================
# TESTING CONFIGURATION
# =============================================================================

# Test timeout in seconds
PYTEST_TIMEOUT=300

# Number of parallel test workers (auto, or specific number)
PYTEST_WORKERS=auto

# Minimum code coverage threshold
COVERAGE_THRESHOLD=90

# Enable slow tests (may require actual TPU hardware)
RUN_SLOW_TESTS=false

# Enable hardware tests (requires TPU device)
RUN_HARDWARE_TESTS=false

# =============================================================================
# DOCKER CONFIGURATION
# =============================================================================

# Docker registry for container images
DOCKER_REGISTRY=ghcr.io

# Docker image name
DOCKER_IMAGE_NAME=edge-tpu-v5-benchmark

# Docker image tag
DOCKER_TAG=latest

# Enable GPU passthrough in Docker (for development)
DOCKER_GPU_ENABLED=false

# =============================================================================
# SECURITY SETTINGS
# =============================================================================

# Enable model signature verification
MODEL_SIGNATURE_VERIFICATION=true

# Allow unsigned models (only for development)
ALLOW_UNSIGNED_MODELS=false

# Enable telemetry (anonymous usage statistics)
TELEMETRY_ENABLED=false

# Telemetry endpoint (only used if telemetry is enabled)
TELEMETRY_ENDPOINT=https://telemetry.edge-tpu-benchmark.org

# =============================================================================
# ADVANCED SETTINGS
# =============================================================================

# Number of threads for parallel processing
NUM_THREADS=auto

# Memory limit for model loading (MB, 0 for unlimited)
MEMORY_LIMIT=0

# Enable experimental features
EXPERIMENTAL_FEATURES=false

# Custom model search paths (colon-separated)
# CUSTOM_MODEL_PATHS=/path/to/models:/another/path

# =============================================================================
# DEBUGGING AND DEVELOPMENT
# =============================================================================

# Enable debug output for specific components (comma-separated)
# DEBUG_COMPONENTS=benchmark,compiler,power,thermal

# Save intermediate files for debugging
SAVE_DEBUG_FILES=false

# Enable performance tracing
PERFORMANCE_TRACING=false

# Trace output directory
TRACE_OUTPUT_DIR=./traces

# Enable memory leak detection
MEMORY_LEAK_DETECTION=false